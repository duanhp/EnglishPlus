#------------------------------------------------------------------------------
VERSION	= BWS.01
#------------------------------------------------------------------------------
!ifndef	ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = UniDLL.dll UniExe.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

UniExe.exe: UniExe.dpr
  $(DCC)

UniDLL.dll: UniDLL.dpr
  $(DCC)


