// Package file for Delphi 6

package frxUniDAC6;

{$I frx.inc}

{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  VCL,
  VCLDB,
  dac60,
  unidac60,
  dacvcl60,
  unidacvcl60,
  frxDAC6,
  frx6,
  frxDB6,
{$IFDEF QBUILDER}
  fqb60,
{$ENDIF}
  fs6;

contains
  frxUniDACComponents in '..\frxUniDACComponents.pas',
  frxUniDACEditor in '..\frxUniDACEditor.pas',
  frxUniDACRTTI in '..\frxUniDACRTTI.pas';

end.
